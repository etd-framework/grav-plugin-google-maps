<script src="{{ url('plugin://google-maps/js/google-maps.js') }}"></script>
<script src="https://maps.googleapis.com/maps/api/js{{ apikeystring }}&callback=initMap" async></script>
<div id="gmap" class="google-map" style="width: {{ width }};height: {{ height }};"
     data-lat="{{ lat }}"
     data-lng="{{ lng }}"
     data-zoom="{{ zoom }}"
     data-scrollwheel="{{ scrollwheel }}"
     data-draggable="{{ draggable }}"
     data-pancontrol="{{ pancontrol }}"
     data-iconurl="{{ iconurl }}"
     data-infowindow="{{ infowindow }}">
</div>